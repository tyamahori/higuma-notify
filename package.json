{
  "name": "higuma",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "lint": "bun run eslint .",
    "lint:fix": "bun run eslint . --fix",
    "format": "bun run prettier --write . --ignore-path .gitignore",
    "format:check": "bun run prettier --check . --ignore-path .gitignore",
    "typecheck": "bun run tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.3",
    "hono": "^4.11.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260113.0",
    "@types/node": "^25.0.7",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/ui": "^4.0.17",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "vitest": "^4.0.17",
    "wrangler": "^4.59.2"
  }
}
