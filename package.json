{
  "name": "higuma",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "lint": "bun run eslint .",
    "lint:fix": "bun run eslint . --fix",
    "format": "bun run prettier --write . --ignore-path .gitignore",
    "format:check": "bun run prettier --check . --ignore-path .gitignore",
    "typecheck": "bun run tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "fast-xml-parser": "^5.2.5",
    "hono": "^4.9.8",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250913.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.6.0",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.40.2"
  }
}
